
#include "Logger.h"
#include <string>
#include <iostream>
#include <iomanip>
using namespace std;


Logger* Logger::instance = 0;

Logger::Logger() {}

Logger::~Logger() {}
 

Logger*
Logger::getInstance()
{
    if (instance == 0)
        instance = new Logger();

    return instance; 
}

void
Logger::info(string msg)
{
    cout<<msg<<endl;
}